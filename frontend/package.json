{
  "name": "projectlocker",
  "version": "1.0.0",
  "description": "Frontend for the projectlocker project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack -d",
    "compile": "webpack -p",
    "dev": "webpack -d --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/react-treebeard/.+\\\\.js$",
      "node_modules/(?!panda-session).+\\\\.js$"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^4.0.0",
    "moxios": "^0.4.0",
    "object.entries": "^1.0.4",
    "react-test-renderer": "^16.8.6",
    "sinon": "^2.3.7",
    "snyk": "^1.192.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.5",
    "can-ndjson-stream": "^1.0.2",
    "css-loader": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lodash": ">=4.17.11",
    "lodash.omit": "^4.5.0",
    "moment": "^2.20.1",
    "raven-js": "^3.24.1",
    "react": "^16.8.6",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-multistep": "^3.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-sortable-table": "^1.3.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.0.0",
    "yarn": "^1.3.2"
  }
}
